{
   "device": {
      "serial": "900001",
      "hwversion": 1
   },
   "interface": {
      "amp": {
         "port": "pipe:/tmp/rustyradio.amp.socket",
         "enabled": 1
      },
      "cat": {
         "port": "pipe:/tmp/rustyradio.cat.socket",
         "enabled": 1
      },
      "display": {
         "type": "nextion",
         "port": "/dev/ttyUSB.nextion"
      }
   },
   "build": {
      "cflags": "-fPIC -g -O1",
      "ldflags": "-g",
      "platform": "posix",
      "toolchain": {
         "prefix": ""
      }
   },
   "debug": {
      "noisy-eeprom": 0,
      "logfile": "/tmp/radio-debug.log",
      "logtype": "file",
      "logmode": "0660",
      "loglevel": "debug",
      "targets": "*"
   },
   "eeprom": {
      "addr": "0x0000",
      "size": 16384,
      "type": "mmap"
   },
   "i2c": {
      "myaddr": "0xa0"
   },
   "features": {
      "cat-kpa500": 1,
      "cat-yaesu": 1,
      "max-bands": 14
   },
   "station": {
      "call": "N0CALL",
      "grid": "AA00aa"
   },
   "owner": {
      "call": "N0CALL",
      "privs": "US/General"
   },
   "pin": {
      "show": 1,
      "master": "012345",
      "reset": "543210"
   },
   "net": {
      "interface": "ether",
      "ip": "10.201.1.100",
      "gw": "10.201.1.1",
      "mask": "255.255.255.0",
      "dns1": "10.201.1.1",
      "dns2": "1.1.1.1",
      "mtu": 1500,
      "vlan": 1583
   },
   "thermal": {
      "encl_alarm": 120.0,
      "encl_max": 135.0,
      "encl_target": 95.0,
      "inlet_alarm": 120.0,
      "inlet_max": 120.0,
      "inlet_target": 110.0,
      "final_alarm": 170.0,
      "final_max": 185.0,
      "final_target": 135.0,
      "lpf_alarm": 125.0,
      "lpf_max": 130.0,
      "lpf_target": 125.0
   },
   "rfdecks": {
      "lowband": {
         "bands": [ "160M", "80M", "60M", "40M", "30M", "20M", "17M", "15M", "12M", "10M" ],
         "minpower": "5W",
         "maxpower": [ "500W", "500W", "500W", "600W", "600W", "600W", "600W", "600W", "600W", "600W" ],
         "powerstep": 10
      },
      "highband": {
         "bands": [ "6M", "2M", "1.25M", "70CM" ],
         "minpower": "5W",
         "maxpower": [ "100W", "100W", "100W", "100W" ],
         "powerstep": 10
      }
   },
   "atu": {
      "atu0": {
         "title": "HF ATU",
         "L": [ 10, 5, 2.5, 1.25, 0.59, 0.39, 0.22, 0.11 ],
         "C": [ 642, 320, 160, 82, 39, 20, 10, 5 ]
      }
   }
}
